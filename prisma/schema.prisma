generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Tag {
  id        String   @id
  createdAt DateTime @default(now())
  updatedAt DateTime
  value     String   @unique @db.VarChar(255)
  User      User[]
}

model User {
  id       String @id
  username String @unique
  Tag      Tag[]
}
